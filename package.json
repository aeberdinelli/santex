    "@aws-cdk/aws-signer": "^1.169.0",
    "@types/got": "^9.6.12",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.7.10",
    "@types/node-fetch": "^2.6.2",
    "@types/prettier": "2.6.0",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.4",
